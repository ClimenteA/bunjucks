{
  "name": "bunjucks",
  "description": "Create html static sites with Bun and Nunjucks",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "binary": "bun build ./index.ts --compile --outfile server",
    "dev": "bun --watch index.ts"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/nunjucks": "^3.2.6",
    "nunjucks": "^3.2.4"
  }
}